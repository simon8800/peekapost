<% if (locals.createUserError) { %>
  <div class="errors-list-container">
    <ul class="errors-list">
      <li class="error-msg"><%= createUserError %></li>
    </ul>
  </div>
<% } %>
<% if (locals.errors) { %>
  <div class="errors-list-container">
    <ul class="errors-list">
      <% errors.forEach(error => { %>
        <li class="error-msg"><%= error.msg %></li>
      <% }) %>
    </ul>
  </div>
<% } %>